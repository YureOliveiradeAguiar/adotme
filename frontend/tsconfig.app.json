{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"baseUrl": "./",
		"outDir": "./out-tsc/app",
		"types": [],
		"paths": {
			"@components/*": ["src/app/components/*"],
  			"@pages/*": ["src/app/pages/*"],
  			"@services/*": ["src/app/services/*"],
  			"@utils/*": ["src/app/utils/*"]
		},
	},
	"include": [
		"src/**/*.ts"
	],
	"exclude": [
		"src/**/*.spec.ts"
	],
}